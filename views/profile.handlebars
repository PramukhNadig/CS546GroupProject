<div class="container py-4">
  <h1>Profile for {{username}}</h1>
  <div class="px-4 py-3 mb-4 bg-light rounded-3">
    <h4 class="text-secondary">Playlists</h4>
    <ul>
      {{#each playlists}}
      <li><a href="/playlists/{{_id}}">{{PlaylistName}}</a></li>
      {{/each}}
    </ul>
    {{!-- <p>Song Reviews: {{songReviews}}</p> --}}
    {{#each songReviews}}
    <div>
      <p>-> Song name: {{this.songName}}</p>
      <p>Rating: {{this.rating}}</p>
      <p>Review Title: {{this.title}}</p>
      <p>Review Comment: {{this.comment}}</p>
    </div>
    {{/each}}
    {{#each albumReviews}}
    <div>
      <p>-> Album name: {{this.albumName}}</p>
      <p>Rating: {{this.rating}}</p>
      <p>Review Title: {{this.title}}</p>
      <p>Review Comment: {{this.comment}}</p>
    </div>
    {{/each}}
  </div>
</div>